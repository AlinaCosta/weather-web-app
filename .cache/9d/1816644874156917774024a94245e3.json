 1000);\r\n\r\n  const dayIndex = date.getDay();\r\n  let day;\r\n\r\n  switch (dayIndex) {\r\n    case 0:\r\n      day = 'Duminica';\r\n      break;\r\n    case 1:\r\n      day = 'Luni';\r\n      break;\r\n    case 2:\r\n      day = 'Marti';\r\n      break;\r\n    case 3:\r\n      day = 'Miercuri';\r\n      break;\r\n    case 4:\r\n      day = 'Joi';\r\n      break;\r\n    case 5:\r\n      day = 'Vineri';\r\n      break;\r\n    case 6:\r\n      day = 'Sambata';\r\n      break;\r\n    default:\r\n      throw new Error('Indexul trebuie sa fie intre 0 si 6.');\r\n  }\r\n  return day;\r\n}\r\n\r\nfunction getMonth(utc) {\r\n  const date = new Date(utc * 1000);\r\n\r\n  const monthIndex = date.getMonth();\r\n  let month;\r\n\r\n  switch (monthIndex) {\r\n    case 0:\r\n      month = 'Ianuarie';\r\n      break;\r\n    case 1:\r\n      month = 'Februarie';\r\n      break;\r\n    case 2:\r\n      month = 'Martie';\r\n      break;\r\n    case 3:\r\n      month = 'Aprilie';\r\n      break;\r\n    case 4:\r\n      month = 'Mai';\r\n      break;\r\n    case 5:\r\n      month = 'Iunie';\r\n      break;\r\n    case 6:\r\n      month = 'Iulie';\r\n      break;\r\n    case 7:\r\n      month = 'August';\r\n      break;\r\n    case 8:\r\n      month = 'Septembrie';\r\n      break;\r\n    case 9:\r\n      month = 'Octombrie';\r\n      break;\r\n    case 10:\r\n      month = 'Noiembrie';\r\n      break;\r\n    case 11:\r\n      month = 'Decembrie';\r\n      break;\r\n    default:\r\n      throw new Error('Indexul trebuie sa fie intre 0 si 11.');\r\n  }\r\n  return month;\r\n}\r\n\r\nfunction getHour(utc) {\r\n  const date = new Date(utc * 1000);\r\n\r\n  let hours = date.getHours();\r\n  let minutes = date.getMinutes();\r\n\r\n  if (hours < 10) {\r\n    hours = '0' + hours;\r\n  }\r\n  if (minutes < 10) {\r\n    minutes = '0' + minutes;\r\n  }\r\n  return hours + ':' + minutes;\r\n}\r\n"},"lineCount":null}},"error":null,"hash":"733b534d84d35f406606c7ee5d353339","cacheData":{"env":{}}}